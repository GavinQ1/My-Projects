Program ortest (input,output);
Var
   h, i , j : integer;
   
 begin
 
h:= 1;
 i:= 2;
 j:= 3;
  if ((h <> j) and (h = j)) then
	
  	write(" true case: " 1)
  	
  else write("false case: " 2);
  
  
  if ((h = j) or (h + i = j)) then
  	
  	write(" true case: " 1)
    	
  else write("false case: " 2)
  
 end.